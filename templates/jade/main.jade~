          .banner
          script(language='javascript')
            var cn_sid   = 'sid';
            // values
            Cookies.set_domain('.sports.ru');
            store_cookie_name.set('user', 'favteam_user');
            store_cookie_name.set('nouser', 'favteam0');
            var sid = Cookies.get( cn_sid );
            var rmsid = Cookies.get('rm_sid');
            if (sid) { store_cookie_type = 'user'; }
            var fav_team_id = Cookies.get( store_cookie_name.get(store_cookie_type) );
            document.observe("dom:loaded", function() {
            if (fav_team_id === '0') { return; }
            if (!fav_team_id && (store_cookie_type == 'user'))
            {
            // get settings from user profile
            new Ajax.Request('/ajax/user/fav_team_news_getcfg.html', {
            method: 'post',
            onComplete: function(response) {
            if (200 == response.status) {
            var text = response.responseText || '{}';
            // TODO : process wrong format
            var data = text.evalJSON();
            tag_id = data.tag_id;
            fav_team_id = tag_id;
            Cookies.set( store_cookie_name.get(store_cookie_type), tag_id,
            cookie_store_time.get(store_cookie_type) );
            }
            process_my_team_block();
            }
            });
            }
            else {
            process_my_team_block();
            }
            });
            function process_my_team_block() {
            if (fav_team_id === '0') { return; }
            if (fav_team_id) {
            getMyTeamNews(fav_team_id);
            $('team_settings_block').hide();
            $('favTeamNews').show();
            return;
            }
            $('hide_favteam_block').show(); // show button
            fetchTeamSettings();
            $('favTeamNews').show();
            }
